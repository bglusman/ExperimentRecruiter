<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :age %>
    <%= f.input :major %>
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :school_id %>
    <%= f.input :notes %>
    <%= f.input :status %>
    <%= f.input :mobile %>
    <%= f.input :sex %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
